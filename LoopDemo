#for loop
obj = [2,3,4,5]
for i in obj :
    print(i*2)  #2 3 4 5
print("***************")

#sum of first  natural  Numbes 1+2+3+4+5=15

summation = 1
for j in range(1,7): #if we want to itirate  range(i,j)-> i to j-1
    summation= summation+j
    print(summation)
print("****************")

#(Start , stop , step)
for k in range(1,10,5):
    print(k)

print("****************")

a = int(input())
if a%2:
    print("not")
